public class Operators {
	int num1 = 30;
	int num2 = 20;
	public static void main(String[] args) {
		Operators op = new Operators();
		int result1 = op.num1 + op.num2;
		int result2 = op.num1 - op.num2;
		float result3 = op.num1 / op.num2;
		int result4 = op.num1 * op.num2;
		int result5 = op.num1 % op.num2;
		int result6 = op.num1 & op.num2;
		int result7 = op.num1 | op.num2;
		System.out.println("The given two Numbers are:- Num1:" + op.num1 + " " + "Num2:" + op.num2);
		System.out.println("Addition of two numbers is:" + result1);
		System.out.println("Subtraction of two numbers is:" + result2);
		System.out.println("Division of two numbers is:" + result3);
		System.out.println("Multiplication is:" + result4);
		System.out.println("Modulus of two numbers is:" + result5);
		System.out.println("Bitwise AND of two numbers is:" + result6);
		System.out.println("Bitwise OR of two numbers is:" + result7);
		
		
	}

}
